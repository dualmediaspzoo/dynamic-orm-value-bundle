{
    "name": "dualmedia/dynamic-orm-value-bundle",
    "description": "Bundle for disabling fields in DoctrineORM",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DualMedia\\DynamicORMValueBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DualMedia\\DynamicORMValueBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "pkly"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.3",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/orm": "^3",
        "doctrine/common": "^3",
        "symfony/framework-bundle": "^6.4 || ^7.3",
        "symfony/property-access": "^6.4 || ^7.3",
        "symfony/uid": "^6.4 || ^7.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "pedrotroller/php-cs-custom-fixer": "^2",
        "phpstan/phpstan": "^2",
        "phpstan/phpstan-phpunit": "^2",
        "spaze/phpstan-disallowed-calls": "^4",
        "phpunit/phpunit": "^12",
        "pkly/phpunit-service-create-trait": "^1.0"
    }
}
